&usbdp_phy1 {
	status = "okay";
};

&u2phy0 {
	status = "okay";
};

&u2phy1 {
	status = "okay";
};

&u2phy0_otg {
	/* Power is missing! */
	// phy-supply = <&vbus_typec>;
	status = "okay";
};

&u2phy1_otg {
	/* Power is missing! */
	// phy-supply = <&vbus_host30_1>;
	status = "okay";
